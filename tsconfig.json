{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./",
    "types": [
      "@simonsmith/cypress-image-snapshot/types",
      "cypress"
    ]
  },
  "include": [
    "cypress/**/*.ts",
    "cypress/support/index.d.ts"
  ]
}
